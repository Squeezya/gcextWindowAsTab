{
  "manifest_version": 2,
  "name": "Open window as TAB",
  "version": "0.1",
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.appway.com/admin/*"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "images/tabs16.png"
  },
  "icons": {
    "16": "images/tabs16.png",
    "128": "images/tabs128.png"
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
